{
  "name": "gcp-express-server",
  "version": "1.0.0",
  "description": "minimal express server to serve angular from static folder",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "start-dev": "npm run compile && node ./dist/index.js",
    "compile": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "gcp-build": "tsc -p ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/connect-history-api-fallback": "^1.3.1",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.17",
    "rimraf": "^2.6.2",
    "typescript": "^3.2.2"
  }
}
